{
    "name": "o0h/phpunit-case-decorator",
    "type": "library",
    "require-dev": {
        "php": "^8.1.0",
        "phpunit/phpunit": "^10.0.0||^11.0.0",
        "friendsofphp/php-cs-fixer": "^v3.52.0",
        "phpstan/phpstan": "^1.10.0",
        "squizlabs/php_codesniffer": "^3.9.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "O0h\\PhpunitCaseDecorator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "O0h\\PhpunitCaseDecorator\\Test\\": "tests/",
            "ExampleApp\\": "example/src/",
            "ExampleApp\\Test\\": "example/tests/"
        }
    },
    "authors": [
        {
            "name": "o0h",
            "email": "dev@o0h.in"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
